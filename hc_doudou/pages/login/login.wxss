.pass {
    position: fixed;
    bottom: 150rpx;
    right: 40rpx;
    width: 100rpx;
    background: rgba(0,0,0,0.5);
    color: #fff;
    font-size: 28rpx;
    height: 50rpx;
    line-height: 50rpx;
    border-radius: 25rpx;
    z-index: 99999;
    text-align: center;
}

.ads {
    width: 100%;
    height: 100%;
}

.notice {
    width: 94%;
    padding: 20rpx 3%;
    background: #fff;
}

.noticead {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}

.Ntitle {
    margin-left: 20rpx;
    max-width: 80%;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.Ntitle view:nth-child(1) {
    font-size: 32rpx;
    color: #393939;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.Ntitle view:nth-child(2) {
    font-size: 28rpx;
    color: #828282;
    max-width: 100%;
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.goodslist {
    width: 96%;
    padding: 16rpx 2% 0;
    background: #f4f4f4;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    flex-wrap: wrap;
}

.noticeL {
    width: 100%;
}

.goods {
    width: 49%;
    background: #fff;
    font-size: 28rpx;
    margin-bottom: 16rpx;
}

.goods image {
    width: 352rpx;
    height: 352rpx;
}

.goodsdetail {
    padding: 20rpx 20rpx 10rpx;
}

.price {
    padding: 16rpx 0 0;
}

.goodsName {
    color: #4d4d4d;
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    line-height: 34rpx;
}

.price view:nth-child(1) {
    color: #f94d4d;
}

.price view:nth-child(2) {
    color: #aeaeae;
    font-size: 24rpx;
}