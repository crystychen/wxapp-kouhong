<view class="between" wx:if="{{cashlists.length>0}}" wx:for="{{cashlists}}" wx:key="index">
    <view class="left flex">
        <image src="{{item.avatar}}"></image>
        <view class="column">
            <view>{{item.nickname}}</view>
            <view>{{item.createtime}}</view>
        </view>
    </view>
    <view class="right">-{{item.money}}</view>
</view>
<view style="width:100%;margin-top:30rpx;font-size:28rpx;color:#8e8e8e;text-align:center;" wx:if="{{cashlists.length==0}}">---没有数据---</view>
