page {
    background: #f4f4f4;
}

.orderstatus {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    flex-wrap: wrap;
    width: 100%;
    background: #fff;
    position: fixed;
    top: 0;
}

image {
    display: block;
}

.item {
    display: inline-block;
    text-align: center;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #6D6D6D;
    border-bottom: 6rpx solid #fff;
}

.sel {
    color: #6D6D6D;
    border-bottom: 6rpx solid #333333;
}

.width1 {
    width: 96%;
}

.width2 {
    width: 20%;
    margin: 0 15%;
}

.width3 {
    width: 29.3%;
    margin: 0 2%;
}

.distribution {
    background: #fff;
    width: 100%;
    font-size: 30rpx;
    color: #545454;
}

.between {
    padding: 16rpx 20rpx;
}

.row image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
}

.rights {
    width: 40rpx;
    height: 40rpx;
}

.row {
    font-size: 28rpx;
    align-items: center;
}

.pay {
    width: 90%;
    margin: 40rpx auto;
    background: #F39801;
    border-radius: 10rpx;
    text-align: center;
    color: #fff;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
}

.detail {
    border-top: 2rpx solid #f3f3f3;
    border-bottom: 2rpx solid #f3f3f3;
    padding: 10rpx 10px;
    font-size: 28rpx;
    height: 100px;
}

.detail>image {
    width: 80px;
    height: 80px;
    background: green;
    margin-right: 10px;
}

.ordernum {
    padding: 30rpx 20rpx;
    font-size: 28rpx;
}

.columns {
    height: 80px;
    display: flex;
    flex-direction: column;
    padding: 10px 0;
}

.title {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 20rpx;
}

.rates {
    width: 25%;
    text-align: center;
    border: 2rpx solid #fff;
    height: 90rpx;
    display: flex;
    font-size: 24rpx;
    flex-direction: column;
    justify-content: space-between;
}

.rate {
    height: 90rpx;
    margin-top: 10rpx;
}

.ratenum {
    width: 100%;
    text-align: center;
    border: 2rpx solid #FEACC1;
    color: #FD1A55;
    border-radius: 6rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.mark {
    height: 90rpx;
    margin: 0 3%;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
}

.mark>view {
    height: 40rpx;
    line-height: 40rpx;
    font-size: 28rpx;
}

.notice {
    height: 340rpx;
    width: 90%;
    margin: 0 5%;
    border-radius: 30rpx;
    background: rgba(252,196,0,0.5);
    color: #fff;
}

.top {
    background: rgba(246,208,90,0.5);
    height: 80rpx;
    line-height: 80rpx;
    border-top-left-radius: 30rpx;
    border-top-right-radius: 30rpx;
    font-size: 30rpx;
    font-weight: blod;
    text-align: center;
    margin-bottom: 20rpx;
}

.items {
    width: 80%;
    padding: 6rpx 10%;
    font-size: 28rpx;
}

.nice {
    color: #000;
    font-size: 30rpx;
    width: 80%;
    padding: 6rpx 10%;
}

.title {
    border-left: 6rpx solid #303030;
    color: #303030;
    font-size: 30rpx;
    font-weight: bold;
    width: 92%;
    margin: 30rpx 4%;
    padding-left: 20rpx;
}

.upmoney>view:nth-child(1) {
    font-size: 30rpx;
}

.upmoney>view:nth-child(2) {
    font-size: 60rpx;
    padding: 0;
    margin: 20rpx 0 30rpx -10rpx;
}

.lines {
    width: 100rpx;
    border-bottom: 2rpx solid #fff;
}

.upmoney {
    color: #fff;
    width: 80%;
    margin: 0 4%;
    border-radius: 14rpx;
    padding: 20rpx 5%;
    height: 200rpx;
    background-size: 100% 100%;
}

.up {
    width: 100%;
    height: 340rpx;
    padding: 30rpx 0;
}