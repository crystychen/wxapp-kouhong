@import "wxParse/wxParse.wxss";

.flex {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.between {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.column {
    display: flex;
    flex-direction: column;
    align-items: center;
}